template BSP_pic32wk_gpb_gpd_sk_module_H "$HARMONY_VERSION_PATH/bsp/pic32wk_gpb_gpd_sk+module/templates/bsp.h.ftl" to "$PROJECT_HEADER_FILES/app/system_config/$CONFIGURATION/bsp/bsp.h"
