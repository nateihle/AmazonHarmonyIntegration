ifblock BSP_PIC32WK_GPB_GPD_SK_MODULE

menu "Select BSP Features"
    depends on USE_BSP
    depends on BSP_PIC32WK_GPB_GPD_SK_MODULE

config BSP_CONFIGURE_PIC32WK_GPB_GPD_SK_MODULE
    depends on USE_BSP
    select BSP_TRIGGER
    select BSP_USE_USBSWITCH
    set BSP_PIN_15_FUNCTION_NAME to "BSP_SWITCH_1" if COMPONENT_PACKAGE = "Module"
    set BSP_PIN_15_FUNCTION_TYPE to "SWITCH" if COMPONENT_PACKAGE = "Module"
    set BSP_PIN_16_FUNCTION_NAME to "BSP_SWITCH_2" if COMPONENT_PACKAGE = "Module"
    set BSP_PIN_16_FUNCTION_TYPE to "SWITCH" if COMPONENT_PACKAGE = "Module"
    set BSP_PIN_25_FUNCTION_NAME to "BSP_SWITCH_3" if COMPONENT_PACKAGE = "Module"
    set BSP_PIN_25_FUNCTION_TYPE to "SWITCH" if COMPONENT_PACKAGE = "Module"
    set BSP_PIN_27_FUNCTION_NAME to "BSP_LED_1" if COMPONENT_PACKAGE = "Module"
    set BSP_PIN_27_FUNCTION_TYPE to "LED_AH"  if COMPONENT_PACKAGE = "Module"
    set BSP_PIN_19_FUNCTION_NAME to "BSP_LED_2" if COMPONENT_PACKAGE = "Module"
    set BSP_PIN_19_FUNCTION_TYPE to "LED_AH" if COMPONENT_PACKAGE = "Module"
    set BSP_PIN_60_FUNCTION_NAME to "BSP_LED_3" if COMPONENT_PACKAGE = "Module"
    set BSP_PIN_60_FUNCTION_TYPE to "LED_AH" if COMPONENT_PACKAGE = "Module"
    set BSP_PIN_52_FUNCTION_NAME to "USB_VBUS_SWITCH" if COMPONENT_PACKAGE = "Module"
    set BSP_PIN_52_FUNCTION_TYPE to "VBUS" if COMPONENT_PACKAGE = "Module"

    set BSP_PIN_70_FUNCTION_NAME to "BSP_SWITCH_1" if COMPONENT_PACKAGE = "DQFN"
    set BSP_PIN_70_FUNCTION_TYPE to "SWITCH" if COMPONENT_PACKAGE = "DQFN"
    set BSP_PIN_71_FUNCTION_NAME to "BSP_SWITCH_2" if COMPONENT_PACKAGE = "DQFN"
    set BSP_PIN_71_FUNCTION_TYPE to "SWITCH" if COMPONENT_PACKAGE = "DQFN"
    set BSP_PIN_77_FUNCTION_NAME to "BSP_SWITCH_3" if COMPONENT_PACKAGE = "DQFN"
    set BSP_PIN_77_FUNCTION_TYPE to "SWITCH" if COMPONENT_PACKAGE = "DQFN"
    set BSP_PIN_11_FUNCTION_NAME to "BSP_LED_1" if COMPONENT_PACKAGE = "DQFN"
    set BSP_PIN_11_FUNCTION_TYPE to "LED_AH" if COMPONENT_PACKAGE = "DQFN"
    set BSP_PIN_130_FUNCTION_NAME to "BSP_LED_2" if COMPONENT_PACKAGE = "DQFN"
    set BSP_PIN_130_FUNCTION_TYPE to "LED_AH" if COMPONENT_PACKAGE = "DQFN"
    set BSP_PIN_93_FUNCTION_NAME to "BSP_LED_3" if COMPONENT_PACKAGE = "DQFN"
    set BSP_PIN_93_FUNCTION_TYPE to "LED_AH" if COMPONENT_PACKAGE = "DQFN"
    set BSP_PIN_86_FUNCTION_NAME to "USB_VBUS_SWITCH" if COMPONENT_PACKAGE = "DQFN"
    set BSP_PIN_86_FUNCTION_TYPE to "VBUS" if COMPONENT_PACKAGE = "DQFN"	
    bool
    ---help---
    BSP for PIC32WK Starter Kit with Module
    ---endhelp---

endmenu


menu "Custom Board Configurations for PIC32WK Starter Kit with Module"
