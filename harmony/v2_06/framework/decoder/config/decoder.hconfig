    config USE_WMA_DECODER
    bool "Use WMA Decoder Library (premium purchase)"
    default n
	---help---
	IDH_HTML_WMA_Decoder_Library
	---endhelp---
    depends on USE_DECODER
        config WMA_DECODER_LIB_SELECT
        string "WMA Library"
